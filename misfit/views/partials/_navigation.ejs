<!-- body -->
<body class="main-layout">
    <!-- loader  -->
    <div class="loader_bg">
       <div class="loader"><img src="/images/loading.gif" alt="#"/></div>
    </div>
    <!-- end loader -->
    <!-- header -->
    <header>
       <!-- header inner -->
       <div class="header">
          <div class="container">
             <div class="row">
                <div class="col-xl-3 col-lg-3 col-md-3 col-sm-3 col logo_section">
                   <div class="full">
                      <div class="center-desk">
                         <div class="logo">
                            <a href="/"><img src="/images/logo.png" alt="#" /></a>
                         </div>
                      </div>
                   </div>
                </div>
                <div class="col-xl-9 col-lg-9 col-md-9 col-sm-9">
                   <nav class="navigation navbar navbar-expand-md navbar-dark ">
                      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExample04" aria-controls="navbarsExample04" aria-expanded="false" aria-label="Toggle navigation">
                      <span class="navbar-toggler-icon"></span>
                      </button>
                      <div class="collapse navbar-collapse" id="navbarsExample04">
                         <ul class="navbar-nav mr-auto">
                            <li class="nav-item">
                               <a class="nav-link <%= page_name === 'index' && 'active' %>" href="/index">Home</a>
                            </li>
                            <li class="nav-item">
                               <a class="nav-link <%= page_name === 'about' && 'active' %>" href="/about">About</a>
                            </li>
                            <li class="nav-item">
                               <a class="nav-link <%= page_name === 'trainer' && 'active' %>" href="/trainer">Trainer</a>
                            </li>
                            <li class="nav-item">
                               <a class="nav-link <%= page_name === 'workouts' && 'active' %>" href="/workouts">Workouts</a>
                            </li>
                            <li class="nav-item">
                               <a class="nav-link <%= page_name === 'contact' && 'active' %>" href="/contact">Contact Us</a>
                            </li>
                            <% if(!userIN) { %> 
                            <li class="nav-item">
                              <a class="nav-link <%= page_name === 'register' && 'active' %>" href="/register">Register</a>
                           </li>
                           <li class="nav-item">
                              <a class="nav-link <%= page_name === 'login' && 'active' %>" href="/login">Login</a>
                           </li>
                           <% } %>
                           <% if(userIN) { %> 
                              <li class="nav-item">
                                <a class="nav-link <%= page_name === 'logout' && 'active' %>" href="/register">Logout</a>
                             </li>
                             <% } %>
                         </ul>
                      </div>
                   </nav>
                </div>
             </div>
          </div>
       </div>
    </header>
    <!-- end header inner -->
    <!-- end header -->
    <!-- banner -->